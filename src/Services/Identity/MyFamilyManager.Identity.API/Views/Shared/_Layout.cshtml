


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>MyFamilyManager IdentityServer</title>
    @*<link rel="stylesheet" href="~/css/vendor.min.css" asp-append-version="true" />*@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.min.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" asp-append-version="true"  />


</head>
<body class="d-flex flex-column min-vh-100">
    <div>
        <nav class="bg-info mb-4 navbar navbar-dark navbar-expand-md">
            <a asp-controller="Home" asp-action="Index" class="navbar-brand">
                MyFamilyManager
            </a>
            <button aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation" class="navbar-toggler" data-target="#topNavbarCollapse" data-toggle="collapse" type="button">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="topNavbarCollapse">
                <ul class="mr-auto navbar-nav">
                    <li class="nav-item">
                        <a asp-controller="Home" asp-action="Index" class="nav-link">HOME</a>
                    </li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li class="nav-item">
                    </li>
                </ul>
                <partial name="_LoginPartial" />


            </div>
        </nav>

    </div>

    <div class="container body-content justify-content-center">
        @RenderBody()
       
    </div>

    <footer>
        <hr />
        <nav class="navbar transparent navbar-inverse">
            <p>&copy; 2020 - MyFamilyManager</p>

        </nav>
    </footer>

    @*<script src="~/js/vendor.min.js" asp-append-version="true"></script>*@
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/jquery/jquery.slim.min.js" asp-append-version="true"></script>
    <script src="~/lib/popper.js/cjs/popper.min.js" asp-append-version="true"></script>
    <script src="~/lib/twitter-bootstrap/js/bootstrap.min.js" asp-append-version="true"></script>


    @RenderSection("scripts", required: false)
</body>
</html>
